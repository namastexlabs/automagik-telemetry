{
  "mcpServers": {
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ]
    },
    "genie": {
      "command": "npx",
      "args": [
        "automagik-genie@latest",
        "mcp"
      ]
    },
    "forge": {
      "command": "npx",
      "args": [
        "automagik-forge",
        "--mcp"
      ]
    }
  }
}